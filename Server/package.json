{
  "name": "the-chat-matrix",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "start-nodemon": "nodemon bin/www",
    "server": "SET DEBUG=the-chat-matrix:* & npm run start-nodemon",
    "watch": "watchify ./client/src/js/chat-main.js ./client/src/js/vip-dashboard-main.js ./client/src/js/vip-main.js ./client/src/js/main.js -p [factor-bundle -o ./public/js/chat-bundle.js -o ./public/js/vip-dashboard-bundle.js -o ./public/js/vip-bundle.js -o ./public/js/bundle.js] -o ./public/js/common.js -v",
    "build:analyze": "set NODE_ENV=production&&set BUNDLE_ANALYZE=true&&webpack --progress -p"
  },
  "proxy": "http://localhost:4000",
  "dependencies": {
    "body-parser": "^1.18.3",
    "canvas": "^2.6.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "ejs": "1.0.0",
    "event-emitter": "^0.3.5",
    "events": "^3.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-mysql-session": "^2.1.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "gulp-eslint": "^6.0.0",
    "gulp-nodemon": "^2.5.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql": "^2.16.0",
    "nocache": "^2.1.0",
    "nodemailer": "^5.0.0",
    "nude": "0.0.1",
    "passport": "^0.4.0",
    "passport-twitter": "1.0.2",
    "pug": "^2.0.3",
    "rand-token": "^0.4.0",
    "react-icons": "^3.9.0",
    "request": "^2.88.0",
    "request-ip": "^2.1.3",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.2.0",
    "stripe": "^6.36.0",
    "url-search-params-polyfill": "^8.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "factor-bundle": "^2.5.0",
    "gulp": "^4.0.2",
    "nodemon": "^1.18.9",
    "watchify": "^3.11.1",
    "webpack-cli": "^3.3.11"
  }
}
